         <empty/>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.milestoneUnit.attributes"/>
         <ref name="teiatt.typed.attributes"/>
         <ref name="teiatt.edition.attributes"/>
         <ref name="teiatt.spanning.attributes"/>
         <ref name="teiatt.breaking.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teigb">
