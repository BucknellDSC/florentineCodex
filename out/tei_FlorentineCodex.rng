               <ref name="teimodel.gLike"/>
               <ref name="teimodel.measureLike"/>
            </choice>
         </zeroOrMore>
         <ref name="teiatt.global.attributes"/>
         <ref name="teiatt.measurement.attributes"/>
         <ref name="teiatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="teiunit">
